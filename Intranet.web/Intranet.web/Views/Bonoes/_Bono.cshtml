@model Intranet.web.Data.Entities.Fidelizacion.Bono

<div class="card-body">

    <div class="form-group">
        <label asp-for="Codigo" class="control-label"></label>
        <input asp-for="Codigo" class="form-control" />
        <span asp-validation-for="Codigo" class="text-danger"></span>
    </div>

    <div class="form-group">
        <label asp-for="Fechacreado" class="control-label"></label>
        <input asp-for="Fechacreado" class="form-control" />
        <span asp-validation-for="Fechacreado" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Valor" class="control-label"></label>
        <input asp-for="Valor" class="form-control" />
        <span asp-validation-for="Valor" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Actividad" class="control-label"></label>
        <input asp-for="Actividad" class="form-control" />
        <span asp-validation-for="Actividad" class="text-danger"></span>

@model Intranet.web.Models.AddIncapacityViewModel
@{
    ViewData["Title"] = "Create";
}


<section class="content-header">
</section>
<section class="content">
    <div class="container-fluid">
        <form asp-action="AddIncapacity">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="row">
                <div class="col-md-12">
                    <div class="card card-dark">
                        <div class="card-header">
                            <h2>Novedades</h2>
                       
                        </div>
                        <div class="card-body">

                            <input type="hidden" asp-for="EmployeeIds" />

                            <partial name="_Incapacity" />
                        </div>
                        <div class="card-footer">
                            <div class="form-group">
                                <input type="submit" value="Guardar" class="btn btn-primary" />
                                <a asp-action="Details" asp-route-id="@Model.EmployeeIds" class="btn btn-success">Volver atras</a>
                            </div>
                        </div>
                    </div>





                </div>
            </div>
        </form>
        </div>
    </section>

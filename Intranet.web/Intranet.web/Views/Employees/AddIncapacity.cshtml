@model Intranet.web.Models.AddIncapacityViewModel
@{
    ViewData["Title"] = "Create";
}
<h2>Create</h2>
<h4>Incapacidad</h4>
<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="AddIncapacity">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="EmployeeIds" />

            <partial name="_Incapacity" />

            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
                <a asp-action="Details" asp-route-id="@Model.EmployeeIds" class="btn btn-success">Back to Employed</a>
            </div>
        </form>
    </div>
</div>
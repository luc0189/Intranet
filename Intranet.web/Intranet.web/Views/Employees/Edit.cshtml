@model Intranet.web.Models.EditEmployedViewModel
@{
    ViewData["Title"] = "Edit";
}

<h2>Edit</h2>

<h4>Employe</h4>
<hr />
<div class="row">
    <form asp-action="Edit">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <input type="hidden" asp-for="Id" />
        <div class="form-group">
            <label asp-for="UserModify" class="control-label"></label>
            <input asp-for="UserModify" />
            <span asp-validation-for="UserModify" class="text-danger"></span>
        </div>

        <partial name="_Employe" />

        <div class="col-md-4">
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
                <a asp-action="Index" class="btn btn-success">Back to List</a>
            </div>
        </div>

    </form>
    
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}


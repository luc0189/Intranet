@model Intranet.web.Models.EditEndowmentVieModel
@{
    ViewData["Title"] = "Edit";
}
<h2>Editar</h2>
<h4>Endowment</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditEndowment">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <input type="hidden" asp-for="Id" />
            <input type="hidden" asp-for="EmployeeId" />

            <<partial name="_Endowment" />


            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-primary" />
                <a asp-action="Details" asp-route-id="@Model.EmployeeId" class="btn btn-success">Back to Site</a>
            </div>
            <div class="panel-footer">
                <div>

                    <input style="border: none;" type="hidden" asp-for="UserRegistra" />

                    <input style="border: none;" type="hidden" asp-for="DateRegistro" />
                    <label asp-for="UserModify"></label>
                    <input style="border: none;" asp-for="UserModify" disabled />
                    <label asp-for="DateModify"></label>
                    <input style="border: none;" asp-for="DateModify" disabled />
                </div>
            </div>
        </form>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
@model Intranet.web.Models.EditIncapacityViewModel
@{
    ViewData["Title"] = "Edit";
}
<section class="content-header">
</section>
<section class="content">
<div class="container-fluid">

  
        <div class="row">
            <div class="col-md-12">
                <form asp-action="EditIncap">
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input type="hidden" asp-for="EmployeeIds" />
                    <input type="hidden" asp-for="Id" />
                    <div class="card card-dark">
                        <div class="card-header">
                            <h2>Editar Incapacidades</h2>
                        </div>
                        <div class="card-body">
                            <partial name="_Incapacity" />
                        </div>
                        <div class="card-footer">
                            <div class="row">
                                <div class="col-9">
                                    <input style="border: none;" type="hidden" asp-for="UserRegistra" />

                                    <input style="border: none;" type="hidden" asp-for="DateRegistro" />
                                    <label asp-for="UserModify"></label>
                                    <input style="border: none;" asp-for="UserModify" disabled />
                                    <label asp-for="DateModify"></label>
                                    <input style="border: none;" asp-for="DateModify" disabled />
                                </div>
                                <div class="col-3">
                                    <div class="text-right">
                                        <div class="form-group">
                                            <input type="submit" value="Actualizar" class="btn btn-primary" />
                                            <a asp-action="Details" asp-route-id="@Model.EmployeeIds" class="btn btn-success">Volver atras</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                          
                        </div>

                    </div>
                 

                    
                </form>
            </div>
        </div>
    </div>
</section>
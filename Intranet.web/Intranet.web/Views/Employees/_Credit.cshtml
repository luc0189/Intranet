@model Intranet.web.Models.CreditViewModel
@{
    ViewData["Title"] = "Create";
}

<div class="form-group">
    <label asp-for="EntityId" class="control-label"></label>
    <select asp-for="EntityId" asp-items="Model.Entityes" class="form-control">
    </select>
    <span asp-validation-for="EntityId" class="text-danger"></span>
</div>

<div class="form-group">
    <label asp-for="NumberL" class="control-label"></label>
    <input asp-for="NumberL" class="form-control" />
    <span asp-validation-for="NumberL" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Quotmonthly" class="control-label"></label>
    <input asp-for="Quotmonthly" class="form-control" />
    <span asp-validation-for="Quotmonthly" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="TotalPrice" class="control-label"></label>
    <input asp-for="TotalPrice" class="form-control" />
    <span asp-validation-for="TotalPrice" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="DeadlinePay" class="control-label"></label>
    <input asp-for="DeadlinePay" class="form-control" />
    <span asp-validation-for="DeadlinePay" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="StartDate" class="control-label"></label>
    <input asp-for="StartDate" class="form-control" />
    <span asp-validation-for="StartDate" class="text-danger"></span>
</div>

<div class="form-group">
    <label asp-for="IsActive" class="control-label"></label>
    <input asp-for="IsActive" class="checked" />
    <span asp-validation-for="IsActive" class="text-danger"></span>
</div>
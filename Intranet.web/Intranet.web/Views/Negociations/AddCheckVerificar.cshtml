@model Intranet.web.Models.Compras.VerificaViewModel
@{
    ViewData["Title"] = "Verificar Negociacion";
}



<form asp-action="AddCheckVerificar">
    <section class="content-header">

</section> 
<section class="content">
    <div class="container-fluid">
    <div class="row">
        <div class="col-md-12">
             <div class="card card-dark">
            <div class="card-header">
                <h2>Verificar Negociacion</h2>
              
            </div>
            <div class="card-body">
               
                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                    <input type="hidden" asp-for="NegociacionId" />
                    <partial name="_Verificado" />
               
            </div>



            <div class="card-footer">
                <div class="form-group">
                    <input type="submit" value="Guardar" class="btn btn-primary" />
                    <a asp-action="Details" asp-route-id="@Model.NegociacionId" class="btn btn-success">Volver a la Negociacion</a>
                </div>
            </div>

        </div>
        </div>
       
    </div>
    </div>
    </section>
</form>

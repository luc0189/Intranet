@model Intranet.web.Models.EditUserViewModel


    <div class="row">
        <div class="col-md-12">
            <div class="col-xs-6 col-sm-3">

                <div class="form-group">
                    <label asp-for="Activo" class="control-label"></label>
                    <input asp-for="Activo" />
                    <span asp-validation-for="Activo" class="text-danger"></span>
                </div>

            </div>
            <div class="col-xs-6 col-sm-3">

                <div class="form-group">
                    <label asp-for="License" class="control-label"></label>
                    <input asp-for="License" />
                    <span asp-validation-for="License" class="text-danger"></span>
                </div>
            </div>
            <div class="col-xs-6 col-sm-3">

                <div class="form-group">
                    <label asp-for="Arl" class="control-label"></label>
                    <input asp-for="Arl" />
                    <span asp-validation-for="Arl" class="text-danger"></span>
                </div>
            </div>
            <div class="col-xs-6 col-sm-3">

                <div class="form-group">
                    <label asp-for="DateRetiro" class="control-label"></label>
                    <input asp-for="DateRetiro"  />
                    <span asp-validation-for="DateRetiro" class="text-danger"></span>
                </div>
            </div>
        </div>
       
    </div>
    <div class="col-md-4">


        <div class="form-group">
            <label asp-for="Document" class="control-label"></label>
            <input asp-for="Document" class="form-control" />

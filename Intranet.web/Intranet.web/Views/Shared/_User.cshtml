            <label asp-for="NivelEducation" class="control-label"></label>
            <input asp-for="NivelEducation" class="form-control" />
            <span asp-validation-for="NivelEducation" class="text-danger"></span>
        </div>
        <div class="form-group">
            <label asp-for="JobTitle" class="control-label"></label>
            <input asp-for="JobTitle" class="form-control" />
            <span asp-validation-for="JobTitle" class="text-danger"></span>
        </div>


